# Admin API Environment Variables
# Copy this file to .env

# Database - Admin DB
ADMIN_DB_HOST=
ADMIN_DB_PORT=5432
ADMIN_DB_NAME=ironhub_admin
ADMIN_DB_USER=
ADMIN_DB_PASSWORD=
ADMIN_DB_SSLMODE=require
ADMIN_DB_CONNECT_TIMEOUT=10
ADMIN_DB_APPLICATION_NAME=ironhub_admin_api

# Authentication
ADMIN_SECRET=your-secure-admin-secret
ADMIN_SESSION_SECRET=your-session-secret
ADMIN_INITIAL_PASSWORD=initial-setup-password

# Neon Database Provisioning
NEON_API_TOKEN=
NEON_PROJECT_ID=
NEON_BRANCH_ID=

# Tenant Configuration
TENANT_BASE_DOMAIN=ironhub.motiona.xyz
TENANT_DB_SUFFIX=_db

# B2 Storage (optional)
B2_BUCKET_NAME=
B2_KEY_ID=
B2_APPLICATION_KEY=
B2_ENDPOINT=

# CORS
ALLOWED_ORIGINS=https://admin.ironhub.motiona.xyz,https://ironhub.motiona.xyz
